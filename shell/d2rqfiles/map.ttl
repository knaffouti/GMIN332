@prefix map: <#> .
@prefix db: <> .
@prefix vocab: <vocab/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .

map:database a d2rq:Database;
	d2rq:jdbcDriver "com.mysql.jdbc.Driver";
	d2rq:jdbcDSN "jdbc:mysql://localhost:3306/rdfdatabase";
	d2rq:username "root";
	d2rq:password "rabah123";
	jdbc:autoReconnect "true";
	jdbc:zeroDateTimeBehavior "convertToNull";
	.

# Table feature
map:feature a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "feature/@@feature.identifier@@";
	d2rq:class vocab:feature;
	d2rq:classDefinitionLabel "feature";
	.
map:feature__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property rdfs:label;
	d2rq:pattern "feature #@@feature.identifier@@";
	.
map:feature_identifier a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_identifier;
	d2rq:propertyDefinitionLabel "feature identifier";
	d2rq:column "feature.identifier";
	d2rq:datatype xsd:integer;
	.
map:feature_name a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_name;
	d2rq:propertyDefinitionLabel "feature name";
	d2rq:column "feature.name";
	.
map:feature_countryCode a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_countryCode;
	d2rq:propertyDefinitionLabel "feature countryCode";
	d2rq:column "feature.countryCode";
	.
map:feature_latitude a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_latitude;
	d2rq:propertyDefinitionLabel "feature latitude";
	d2rq:column "feature.latitude";
	d2rq:datatype xsd:double;
	.
map:feature_longitude a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_longitude;
	d2rq:propertyDefinitionLabel "feature longitude";
	d2rq:column "feature.longitude";
	d2rq:datatype xsd:double;
	.
map:feature_population a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_population;
	d2rq:propertyDefinitionLabel "feature population";
	d2rq:column "feature.population";
	d2rq:datatype xsd:integer;
	.
map:feature_deptCode__ref a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_deptCode;
	d2rq:refersToClassMap map:inseedept;
	d2rq:join "feature.deptCode => inseedept.deptCode";
	.
map:feature_inseeCode__ref a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:feature;
	d2rq:property vocab:feature_inseeCode;
	d2rq:refersToClassMap map:inseecom;
	d2rq:join "feature.inseeCode => inseecom.inseeCode";
	.

